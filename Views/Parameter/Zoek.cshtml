@model PSMS_Beheer.Models.ParameterZonderSubtypeZoekViewModel

@using (Html.BeginForm())
{
    <div>
        Zoek:
        <div>
            @Html.EditorFor(m => m.Zoek)
        </div>
    </div>
            <div>
                <table class="table-striped">
                    <tr>
                        <th> Parameternaam </th>
                        <th> Beschrijving kort </th>
                        <th> Waarde </th>
                        <th> Datatype </th>
                        <th> Domeinnaam </th>
                        <th> Geldig van </th>
                        <th> Geldig tot </th>
                    </tr>
            @foreach (var m in  Model.para)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(mItem => m.parameternaam) 
                    </td>
                    <td>
                        @Html.DisplayFor(mItem => m.beschrijving_kort)
                    </td>
                    <td>
                        @Html.DisplayFor(mItem => m.waarde)
                    </td>
                    <td>
                      waarde
                    </td>
                    <td>

                    </td>
                   <td>
                        @Html.DisplayFor(mItem => m.geldigheidvan)
                    </td>
                    <td>
                        @Html.DisplayFor(mItem => m.geldigheidtot)
                    </td>
                </tr>
            }
                </table>
            </div>

            <div class="form-group">
                <div class="col-md-offset-2 col-md-10">
                    <input type="submit" value="Create" class="btn btn-default" />
                </div>
            </div>

}

